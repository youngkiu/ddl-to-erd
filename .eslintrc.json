{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": [
            "off"
        ],
        "no-console": [
            "error", { "allow": ["warn", "error"] }
        ]
    }
}
